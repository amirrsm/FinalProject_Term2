<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="CENTER" hgap="30.0" prefHeight="750.0" prefWidth="1400.0" styleClass="MainBackground"
          stylesheets="@../Css/StoryPage.css" vgap="30.0" xmlns="http://javafx.com/javafx/11.0.1"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="Interpreter.Graphics.Controller.StoryPageController">
    <columnConstraints>
        <ColumnConstraints/>
        <ColumnConstraints/>
        <ColumnConstraints/>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
    </rowConstraints>
    <Button fx:id="stage1" mnemonicParsing="false" onAction="#onStoryButtons" onMouseEntered="#mouseEnter"
            onMouseExited="#mouseExit" opacity="0.65" prefHeight="175.0" prefWidth="300.0" styleClass="Stage1"
            stylesheets="@../Css/StoryPage.css"/>
    <Button fx:id="stage2" mnemonicParsing="false" onAction="#onStoryButtons" onMouseEntered="#mouseEnter"
            onMouseExited="#mouseExit" opacity="0.65" prefHeight="175.0" prefWidth="300.0" styleClass="Stage2"
            stylesheets="@../Css/StoryPage.css" GridPane.columnIndex="1"/>
    <Button fx:id="stage3" mnemonicParsing="false" onAction="#onStoryButtons" onMouseEntered="#mouseEnter"
            onMouseExited="#mouseExit" opacity="0.65" prefHeight="175.0" prefWidth="300.0" styleClass="Stage3"
            stylesheets="@../Css/StoryPage.css" GridPane.columnIndex="2"/>
    <Button fx:id="stage4" mnemonicParsing="false" onAction="#onStoryButtons" onMouseEntered="#mouseEnter"
            onMouseExited="#mouseExit" opacity="0.65" prefHeight="175.0" prefWidth="300.0" styleClass="Stage4"
            stylesheets="@../Css/StoryPage.css" GridPane.rowIndex="1"/>
    <Button fx:id="stage5" mnemonicParsing="false" onAction="#onStoryButtons" onMouseEntered="#mouseEnter"
            onMouseExited="#mouseExit" opacity="0.65" prefHeight="175.0" prefWidth="300.0" styleClass="Stage5"
            stylesheets="@../Css/StoryPage.css" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    <Button fx:id="stage6" mnemonicParsing="false" onAction="#onStoryButtons" onMouseEntered="#mouseEnter"
            onMouseExited="#mouseExit" opacity="0.65" prefHeight="175.0" prefWidth="300.0" styleClass="Stage6"
            stylesheets="@../Css/StoryPage.css" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
    <Button fx:id="stage7" mnemonicParsing="false" onAction="#onStoryButtons" onMouseEntered="#mouseEnter"
            onMouseExited="#mouseExit" opacity="0.65" prefHeight="175.0" prefWidth="300.0" styleClass="Stage7"
            stylesheets="@../Css/StoryPage.css" GridPane.rowIndex="2"/>
    <Button fx:id="stage8" mnemonicParsing="false" onAction="#onStoryButtons" onMouseEntered="#mouseEnter"
            onMouseExited="#mouseExit" opacity="0.65" prefHeight="175.0" prefWidth="300.0" styleClass="Stage8"
            stylesheets="@../Css/StoryPage.css" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
    <Button fx:id="stage9" mnemonicParsing="false" onAction="#onStoryButtons" onMouseEntered="#mouseEnter"
            onMouseExited="#mouseExit" opacity="0.65" prefHeight="175.0" prefWidth="300.0" styleClass="Stage9"
            stylesheets="@../Css/StoryPage.css" GridPane.columnIndex="2" GridPane.rowIndex="2"/>
    <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.rowIndex="3">
        <children>
            <Button fx:id="back" mnemonicParsing="false" onAction="#onStoryButtons" onMouseEntered="#mouseEnter"
                    onMouseExited="#mouseExit" opacity="0.55" prefHeight="75.0" prefWidth="150.0" styleClass="Back"
                    stylesheets="@../Css/StoryPage.css"/>
        </children>
    </HBox>
</GridPane>
