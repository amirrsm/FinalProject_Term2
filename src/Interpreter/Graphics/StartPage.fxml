<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>


<BorderPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Interpreter.Graphics.StartPageController">
    <center>
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="30.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="compile" mnemonicParsing="false" text="Compile" onAction="#handleStartButtons"/>
                <Button fx:id="story" mnemonicParsing="false" text="Story" onAction="#handleStartButtons"/>
                <Button fx:id="setting" mnemonicParsing="false" text="Setting" onAction="#handleStartButtons"/>
                <Button fx:id="exit" mnemonicParsing="false" text="Exit" onAction="#handleStartButtons"/>
            </children>
        </VBox>
    </center>
    <top>
        <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true"
                   BorderPane.alignment="CENTER"/>
    </top>
</BorderPane>
