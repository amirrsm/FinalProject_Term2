<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.media.MediaView?>
<Pane prefHeight="750.0" prefWidth="1400.0" styleClass="MainBG" stylesheets="@../Css/QuestionPage.css" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="Interpreter.Graphics.Controller.QuestionPageController">

    <TextArea fx:id="codingBox" layoutX="70.0" layoutY="135.0" prefHeight="500.0" prefWidth="260.0" styleClass="CodingBox"
              stylesheets="@../Css/QuestionPage.css"/>
    <ImageView fx:id="questionBox" fitHeight="490.0" fitWidth="445.0" layoutX="490.0" layoutY="150.0" pickOnBounds="true" preserveRatio="true"/>
    <TextArea fx:id="terminalBox" editable="false" layoutX="1070.0" layoutY="220.0" prefHeight="280.0" prefWidth="260.0"
              style="-fx-font-family: century; -fx-font-size: 20;"/>
    <Button fx:id="run" layoutX="1125.0" layoutY="515.0" mnemonicParsing="false" onAction="#onQuestionButtons" prefHeight="40.0" prefWidth="150.0"
            styleClass="Run" stylesheets="@../Css/QuestionPage.css"/>
    <Button fx:id="back" layoutX="1180.0" layoutY="640.0" mnemonicParsing="false" onAction="#onQuestionButtons" onMouseEntered="#mouseEnter"
            onMouseExited="#mouseExit" opacity="0.55" prefHeight="75.0" prefWidth="150.0" styleClass="Back" stylesheets="@../Css/QuestionPage.css"/>
    <MediaView fx:id="mediaView" fitHeight="600.0" fitWidth="600.0" layoutX="400.0" layoutY="80.0" visible="false"/>
</Pane>
