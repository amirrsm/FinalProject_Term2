<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane onMouseMoved="#refresh" prefHeight="750.0" prefWidth="1400.0" styleClass="MainBG" stylesheets="@../Css/LoginPage.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Interpreter.Graphics.Controller.LoginPageController">
    <children>
        <VBox alignment="TOP_CENTER" layoutX="523.0" layoutY="300.0" prefHeight="340.0" prefWidth="380.0" spacing="50.0">
            <children>
                <TextField fx:id="username" alignment="CENTER" prefHeight="30.0" promptText="Username" style="-fx-background-radius: 10;" />
                <TextField fx:id="password" alignment="CENTER" prefHeight="30.0" promptText="Password" style="-fx-background-radius: 10;" />
                <Button fx:id="login" mnemonicParsing="false" onAction="#onLoginButtons" prefHeight="75.0" prefWidth="150.0" styleClass="Login" stylesheets="@../Css/LoginPage.css" />
            </children>
            <padding>
                <Insets left="40.0" right="40.0" top="80.0" />
            </padding>
        </VBox>
        <Button fx:id="back" layoutX="1170.0" layoutY="615.0" mnemonicParsing="false" onAction="#onLoginButtons" onMouseEntered="#mouseEnter" onMouseExited="#mouseExit" prefHeight="75.0" prefWidth="150.0" style="-fx-opacity: 0.55;" styleClass="Back" stylesheets="@../Css/LoginPage.css" />
    </children>
</Pane>
